#information contained in your ServiceNow request
OPENID_CLIENT_ID=
OPENID_CLIENT_SECRET=
OPENID_CALLBACK=

#provided by heroku when the Redis plugin is installed
REDIS_URL=

#required to encrypt the session data in Redis
REDIS_SECRET=

#configurable parameters
AUTH_ENABLED=true
OPENID_SCOPE=openid auth_web
OPENID_ISSUER=https://federate-qa.xh1.lilly.com
PING_VERSION=-qa
USERID_ENDPOINT=/auth/userid
ENABLE_AUTH_LOGGING=false
COOKIE_SECRET=secret (will auto-gen if none is supplied)

